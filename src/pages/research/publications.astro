    background-repeat: no-repeat;
    position: left top;
    color: #1d809c;
    padding-left: 1.5em;
  }

  /* Timeline dates */
  .date-list dl {
    display: inline-block;
    clear: left;
    border-bottom: 1px solid #ccc;
    margin-bottom: 1em;
    padding-bottom: 0.5em;
    min-height: 4em;
  }

  .date-list dt {
    font-size: 4em;
    color: #ccc;
    float: left;
    margin: 0 0.25em 0 0;
    padding: 0;
    line-height: 0.8em;
    position: relative;
  }

  .date-list dd {
    margin: 0 0 0 10em;
    line-height: 1.6em;
  }

  .disc {
    padding-left: 0.75em;
    background-image: url(../images/template_images/black-bullet.png);
    background-repeat: no-repeat;
    background-position: 0px 10px;
  }
</style>
